{
  "name": "redux-status-helpers",
  "version": "5.0.0",
  "description": "Structure Redux Better with Different Statuses",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "scripts": {
    "build": "babel src -d lib --extensions '.ts'",
    "prepublish": "npm run lint && npm run test && npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxshco/redux-status-helpers.git"
  },
  "keywords": [
    "redux",
    "javascript",
    "status",
    "success",
    "failure",
    "cancelled",
    "es6"
  ],
  "author": "Josh Sturrock <jxshco@gmail.com>",
  "bugs": {
    "url": "https://github.com/jxshco/redux-status-helpers/issues"
  },
  "homepage": "https://github.com/jxshco/redux-status-helpers#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/tests/*.test.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
